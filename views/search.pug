extends layouts/main
include mixins/videoBlock

block content
    .search__header
        p Searching for: #{searchingBy}
    .search__videos
        if videos.length === 0
            h4 Not found Video
        each video in videos
            +videoBlock({
                title: video.title,
                views: video.views,
                videoFile: video.videoFile,
                id: video.id
            })